{
  "extends": ["tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]    
  },
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js":false,
    "jsx-no-string-ref": false
  }
}
